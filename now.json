{
    "version": 2,
    "name": "Nestjs APIs",
    "alias": "nestjs-apis.now.sh",
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ]
}